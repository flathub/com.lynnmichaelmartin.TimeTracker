{
    "id" : "com.lynnmichaelmartin.TimeTracker",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "48",
    "sdk" : "org.gnome.Sdk",
    "sdk-extensions" : [
        "org.freedesktop.Sdk.Extension.node22",
        "org.freedesktop.Sdk.Extension.typescript"
    ],
    "command" : "com.lynnmichaelmartin.TimeTracker",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--filesystem=home"
    ],
    "build-options" : {
        "append-path" : "/usr/lib/sdk/typescript/bin:/usr/lib/sdk/node20/bin"
    },
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "time-tracker",
            "builddir" : true,
            "buildsystem" : "meson",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/elvishcraftsman/time-tracker.git",
                    "tag" : "v2.1.5",
                    "commit" : "ffeac6b8fd311357017f4c3a3033d7565b9c4b8d"
                }
            ]
        }
    ]
}
